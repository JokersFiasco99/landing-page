{
  "setup": {
    "command": "npm run setup || npm install --no-fund --no-audit && cp .env.example .env && npm run build"
  },
  "environment": {
    "runtime": "nodejs",
    "version": "18.x"
  },
  "project": {
    "name": "Neumorphic Landing Page",
    "framework": "nextjs",
    "dependencies": [
      "nextjs",
      "tailwindcss",
      "shadcn-ui"
    ]
  },
  "troubleshooting": {
    "npmErrors": [
      "Try installing with --force if there are dependency conflicts",
      "Check Node.js version (14+ required)",
      "Ensure the setup script has execute permissions"
    ]
  }
} 